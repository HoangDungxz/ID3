@model ID3.Models.Humididity

@{
    ViewBag.Title = "Index";
}

<h2>Weather Focast</h2>


<form method="post" action="#">
    @Html.AntiForgeryToken()

    <div class="col-md-6">
        <div class="form-horizontal">

            <div class="form-group">
                <label class="control-label col-md-2">Precipitation</label>
                <div class="col-md-10">
                    <input type="number" class="form-control" name="Precipitation"/>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-md-2">Humididity</label>
                <div class="col-md-10">
                    <input type="number" class="form-control" name="Humidity"/>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-md-2">Temperature</label>
                <div class="col-md-10">
                    <input type="number" class="form-control" name="Temperature" />
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-md-2">Wind</label>
                <div class="col-md-10">
                    <input type="number" class="form-control" name="Wind" />
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Dự đoán" class="btn btn-default" name="Wind"/>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="result">
            <h3>Kết Quả:<span> Sunny</span></h3>
        </div>
    </div>
</form>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
